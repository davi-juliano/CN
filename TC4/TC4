","w = np.append(w, 38)\n","\n","w = np.sort(w)\n","\n","v = np.zeros(w.size)\n","for i in range(w.size):\n","  for j in range(p.size):\n","    v[i] += (w[i]**j)*p[j]\n","\n","print(\"\\nW: \", w)\n","print(\"\\nV: \", v)\n","\n","plt.plot(w, v, 'b', label='3.1', color='r')\n","plt.plot(w, v, 'p', label='3.1', color='b')\n","\n","plt.savefig(\"ex1.png\")"],"execution_count":0,"outputs":[{"output_type":"stream","text":["[1226. 1498. 1822. 2138. 2662. 2840.]\n","[[ 1.22600000e+03  0.00000000e+00  0.00000000e+00  0.00000000e+00\n","   0.00000000e+00  0.00000000e+00]\n"," [ 1.49800000e+03  5.44000000e+01  0.00000000e+00  0.00000000e+00\n","   0.00000000e+00  0.00000000e+00]\n"," [ 1.82200000e+03  6.48000000e+01  1.04000000e+00  0.00000000e+00\n","   0.00000000e+00  0.00000000e+00]\n"," [ 2.13800000e+03  6.32000000e+01 -1.60000000e-01 -8.00000000e-02\n","   0.00000000e+00  0.00000000e+00]\n"," [ 2.66200000e+03  5.24000000e+01 -7.20000000e-01 -2.80000000e-02\n","   2.08000000e-03  0.00000000e+00]\n"," [ 2.84000000e+03  1.78000000e+01 -1.73000000e+00 -4.04000000e-02\n","  -4.13333333e-04 -7.12380952e-05]]\n","\n","Polinômio interpolador do conjunto de pontos dados:   [ 1.12925714e+03 -1.06257143e+01  7.57523810e+00 -3.53190476e-01\n","  7.77904762e-03 -7.12380952e-05]\n","\n","W:  [ 5. 10. 12. 15. 20. 25. 30. 38. 40.]\n","\n","V:  [1226.         1498.         1625.849728   1822.         2138.\n"," 2422.54285714 2662.         2859.665792   2840.        ]\n"],"name":"stdout"},{"output_type":"display_data","data":{"image/png":"iVBORw0KGgoAAAANSUhEUgAAAYAAAAD8CAYAAAB+UHOxAAAABHNCSVQICAgIfAhkiAAAAAlwSFlz\nAAALEgAACxIB0t1+/AAAADl0RVh0U29mdHdhcmUAbWF0cGxvdGxpYiB2ZXJzaW9uIDMuMC4zLCBo\ndHRwOi8vbWF0cGxvdGxpYi5vcmcvnQurowAAIABJREFUeJzt3Xt8VdWZ//HPI0GqRoNIvAyXAW2s\nAlWkEbWprZfKTRG1FbEdpEKbUdERR4uXmQ4Wpz9HRay+Rm1REWmpASs6qCiionhDCRZQQA1eIVCg\nXASjckme3x9rgwck5CQ5yT6X7/v1Oi/OWWefw3P2C9az99prr8fcHRERyT17xR2AiIjEQwlARCRH\nKQGIiOQoJQARkRylBCAikqOUAEREcpQSgIhIjlICEBHJUUoAIiI5Ki/uAPakbdu23ql